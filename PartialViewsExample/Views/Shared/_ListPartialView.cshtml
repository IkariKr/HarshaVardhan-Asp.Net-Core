<div class="list-container">
    <h3>@ViewData["ListTitle"]</h3>
    
    @{
        ViewBag.ListTitle ="Updated Cities";
    }
    
    
    <ul class="list">
        @foreach (string item in ViewBag.ListItems)
        {
            <li>
                @item
            </li>
        }
    </ul>
</div>
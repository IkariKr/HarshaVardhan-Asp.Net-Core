@model IEnumerable<string> 
@inject ICitiesService CitiesServiceInView
@{
    ViewBag.Title = "Home";
}

<h1>Cities</h1>
<ul class="list">
    @foreach(string city in Model)
    {
        <li>@city</li>
    }
</ul>

<p>
    InstanceId of CitiesService: @ViewBag.InstanceScopeCitiesServiceId
</p>

<p>
    InstanceId of CitiesServiceInView: @CitiesServiceInView.ServiceInstanceId
</p>



